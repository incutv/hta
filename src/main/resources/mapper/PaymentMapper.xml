<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.settlementsys.mapper.PaymentMapper">

    <select id="selectPaymentsBetweenDates" resultType="com.example.settlementsys.dto.Payment">
        SELECT *
        FROM payments
        WHERE payment_date BETWEEN #{startDate} AND #{endDate}
    </select>

</mapper>
